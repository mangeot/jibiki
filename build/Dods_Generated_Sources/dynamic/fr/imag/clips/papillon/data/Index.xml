
<TABLE>
    <PACKAGE>fr.imag.clips.papillon.data</PACKAGE>
    <AUTHOR>NN</AUTHOR>
    <PROJECT_NAME>DODS Project</PROJECT_NAME>
    <TABLE_NAME>index</TABLE_NAME>
    <CLASS_NAME>Index</CLASS_NAME>
    <DB_VENDOR>PostgreSQL</DB_VENDOR>
    <TEMPLATE_SET>papillon_dynamic</TEMPLATE_SET>
    <GENERATE_SECURE>false</GENERATE_SECURE>
    <GENERATE_INSECURE>true</GENERATE_INSECURE>
    <MASS_UPDATES>false</MASS_UPDATES>
    <MASS_DELETES>false</MASS_DELETES>
    <DO_IS_OID_BASED>true</DO_IS_OID_BASED>
    <IS_ABSTRACT>false</IS_ABSTRACT>
    <DELETE_CASCADES>false</DELETE_CASCADES>
    <DO_IS_MULTIDB_BASED>false</DO_IS_MULTIDB_BASED>
    <IS_ANY_COLUMN_SECURE>false</IS_ANY_COLUMN_SECURE>
    <GENERATE_DIRTY>Deprecate</GENERATE_DIRTY>

    <COLUMN name="key">
        <IS_CONSTANT>false</IS_CONSTANT>
      <JAVADOC>/**
 * the name of the key
 */</JAVADOC>
        <DB_TYPE>VARCHAR</DB_TYPE>
        <JAVA_TYPE>String</JAVA_TYPE>
        <USED_FOR_QUERY>true</USED_FOR_QUERY>
        <CAN_BE_NULL>true</CAN_BE_NULL>
        <IS_PRIMARY_KEY>false</IS_PRIMARY_KEY>
        <SIZE>255</SIZE>
        <IS_ARRAY>false</IS_ARRAY>
        <GENERATE_SECURE>false</GENERATE_SECURE>
        <GENERATE_INSECURE>true</GENERATE_INSECURE>
    </COLUMN>


    <COLUMN name="lang">
        <IS_CONSTANT>false</IS_CONSTANT>
      <JAVADOC>/**
 * the language of the value of the key
 */</JAVADOC>
        <DB_TYPE>VARCHAR</DB_TYPE>
        <JAVA_TYPE>String</JAVA_TYPE>
        <USED_FOR_QUERY>true</USED_FOR_QUERY>
        <CAN_BE_NULL>true</CAN_BE_NULL>
        <IS_PRIMARY_KEY>false</IS_PRIMARY_KEY>
        <SIZE>3</SIZE>
        <IS_ARRAY>false</IS_ARRAY>
        <GENERATE_SECURE>false</GENERATE_SECURE>
        <GENERATE_INSECURE>true</GENERATE_INSECURE>
    </COLUMN>


    <COLUMN name="value">
        <IS_CONSTANT>false</IS_CONSTANT>
      <JAVADOC>/**
 * the value of the key
 */</JAVADOC>
        <DB_TYPE>VARCHAR</DB_TYPE>
        <JAVA_TYPE>String</JAVA_TYPE>
        <USED_FOR_QUERY>true</USED_FOR_QUERY>
        <CAN_BE_NULL>true</CAN_BE_NULL>
        <IS_PRIMARY_KEY>false</IS_PRIMARY_KEY>
        <SIZE>255</SIZE>
        <IS_ARRAY>false</IS_ARRAY>
        <GENERATE_SECURE>false</GENERATE_SECURE>
        <GENERATE_INSECURE>true</GENERATE_INSECURE>
    </COLUMN>


    <COLUMN name="msort">
        <IS_CONSTANT>false</IS_CONSTANT>
      <JAVADOC>/**
 * plpsql function: multilingual_sort(lang,value)
 */</JAVADOC>
        <DB_TYPE>VARCHAR</DB_TYPE>
        <JAVA_TYPE>String</JAVA_TYPE>
        <USED_FOR_QUERY>true</USED_FOR_QUERY>
        <CAN_BE_NULL>true</CAN_BE_NULL>
        <IS_PRIMARY_KEY>false</IS_PRIMARY_KEY>
        <SIZE>255</SIZE>
        <IS_ARRAY>false</IS_ARRAY>
        <GENERATE_SECURE>false</GENERATE_SECURE>
        <GENERATE_INSECURE>true</GENERATE_INSECURE>
    </COLUMN>


    <COLUMN name="entryId">
        <IS_CONSTANT>false</IS_CONSTANT>
      <JAVADOC>/**
 * the entry id
 */</JAVADOC>
        <DB_TYPE>DECIMAL (19,0)</DB_TYPE>
        <JAVA_TYPE>int</JAVA_TYPE>
        <USED_FOR_QUERY>true</USED_FOR_QUERY>
        <CAN_BE_NULL>false</CAN_BE_NULL>
        <IS_PRIMARY_KEY>false</IS_PRIMARY_KEY>
        <IS_ARRAY>false</IS_ARRAY>
        <GENERATE_SECURE>false</GENERATE_SECURE>
        <GENERATE_INSECURE>true</GENERATE_INSECURE>
    </COLUMN>


    <INDEX id="key_idx" unique="false" clustered="false">
        <INDEX_COLUMN id="key"/>
        <INDEX_COLUMN id="lang"/>
        <INDEX_COLUMN id="value"/>
    </INDEX>

    <INDEX id="entryid_idx" unique="false" clustered="false">
        <INDEX_COLUMN id="entryId"/>
    </INDEX>

    <INDEX id="msort_idx" unique="false" clustered="false">
        <INDEX_COLUMN id="msort"/>
    </INDEX>


</TABLE>
