    <COLUMN name="type">
        <IS_CONSTANT>false</IS_CONSTANT>
      <JAVADOC>/**
 * type of the dictionary
 * values taken from dml:dictType type
 * (monodirectional, bidirectional, pivot, mixed)
 */</JAVADOC>
        <DB_TYPE>VARCHAR</DB_TYPE>
        <JAVA_TYPE>String</JAVA_TYPE>
        <USED_FOR_QUERY>true</USED_FOR_QUERY>
