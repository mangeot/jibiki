
<TABLE>
    <PACKAGE>fr.imag.clips.papillon.data</PACKAGE>
    <AUTHOR>NN</AUTHOR>
    <PROJECT_NAME>DODS Project</PROJECT_NAME>
    <TABLE_NAME>contributionlog</TABLE_NAME>
    <CLASS_NAME>ContributionLog</CLASS_NAME>
    <DB_VENDOR>PostgreSQL</DB_VENDOR>
    <TEMPLATE_SET>standard</TEMPLATE_SET>
    <GENERATE_SECURE>false</GENERATE_SECURE>
    <GENERATE_INSECURE>true</GENERATE_INSECURE>
    <MASS_UPDATES>false</MASS_UPDATES>
    <MASS_DELETES>false</MASS_DELETES>
    <DO_IS_OID_BASED>true</DO_IS_OID_BASED>
    <IS_ABSTRACT>false</IS_ABSTRACT>
    <DELETE_CASCADES>false</DELETE_CASCADES>
    <DO_IS_MULTIDB_BASED>false</DO_IS_MULTIDB_BASED>
    <IS_ANY_COLUMN_SECURE>false</IS_ANY_COLUMN_SECURE>
    <GENERATE_DIRTY>Deprecate</GENERATE_DIRTY>

    <COLUMN name="author">
        <IS_CONSTANT>false</IS_CONSTANT>
      <JAVADOC>/**
 * the login of the author of the contribution
 */</JAVADOC>
        <DB_TYPE>VARCHAR</DB_TYPE>
        <JAVA_TYPE>String</JAVA_TYPE>
        <USED_FOR_QUERY>true</USED_FOR_QUERY>
        <CAN_BE_NULL>false</CAN_BE_NULL>
        <IS_PRIMARY_KEY>false</IS_PRIMARY_KEY>
        <SIZE>255</SIZE>
        <IS_ARRAY>false</IS_ARRAY>
        <GENERATE_SECURE>false</GENERATE_SECURE>
        <GENERATE_INSECURE>true</GENERATE_INSECURE>
    </COLUMN>


    <COLUMN name="groups">
        <IS_CONSTANT>false</IS_CONSTANT>
      <JAVADOC>/**
 * the groups allowed to view the contribution
 */</JAVADOC>
        <DB_TYPE>VARCHAR</DB_TYPE>
        <JAVA_TYPE>String</JAVA_TYPE>
        <USED_FOR_QUERY>true</USED_FOR_QUERY>
        <CAN_BE_NULL>false</CAN_BE_NULL>
        <IS_PRIMARY_KEY>false</IS_PRIMARY_KEY>
        <SIZE>255</SIZE>
        <IS_ARRAY>false</IS_ARRAY>
        <GENERATE_SECURE>false</GENERATE_SECURE>
        <GENERATE_INSECURE>true</GENERATE_INSECURE>
    </COLUMN>


    <COLUMN name="volume">
        <IS_CONSTANT>false</IS_CONSTANT>
      <JAVADOC>/**
 * the volume where the contribution has to be inserted
 */</JAVADOC>
        <DB_TYPE>VARCHAR</DB_TYPE>
        <JAVA_TYPE>String</JAVA_TYPE>
        <USED_FOR_QUERY>true</USED_FOR_QUERY>
        <CAN_BE_NULL>false</CAN_BE_NULL>
        <IS_PRIMARY_KEY>false</IS_PRIMARY_KEY>
        <SIZE>255</SIZE>
        <IS_ARRAY>false</IS_ARRAY>
        <GENERATE_SECURE>false</GENERATE_SECURE>
        <GENERATE_INSECURE>true</GENERATE_INSECURE>
    </COLUMN>


    <COLUMN name="sourceLanguage">
        <IS_CONSTANT>false</IS_CONSTANT>
      <JAVADOC>/**
 * the source language of the contribution
 * values taken from dml:lang type (mainly ISO 639-2/T)
 * (eng, fra, jpn, etc.)
 */</JAVADOC>
        <DB_TYPE>VARCHAR</DB_TYPE>
        <JAVA_TYPE>String</JAVA_TYPE>
        <USED_FOR_QUERY>false</USED_FOR_QUERY>
        <CAN_BE_NULL>false</CAN_BE_NULL>
        <IS_PRIMARY_KEY>false</IS_PRIMARY_KEY>
        <SIZE>255</SIZE>
        <IS_ARRAY>false</IS_ARRAY>
        <GENERATE_SECURE>false</GENERATE_SECURE>
        <GENERATE_INSECURE>true</GENERATE_INSECURE>
    </COLUMN>


    <COLUMN name="headword">
        <IS_CONSTANT>false</IS_CONSTANT>
      <JAVADOC>/**
 * the headword
 */</JAVADOC>
        <DB_TYPE>VARCHAR</DB_TYPE>
        <JAVA_TYPE>String</JAVA_TYPE>
        <USED_FOR_QUERY>true</USED_FOR_QUERY>
        <CAN_BE_NULL>false</CAN_BE_NULL>
        <IS_PRIMARY_KEY>false</IS_PRIMARY_KEY>
        <SIZE>255</SIZE>
        <IS_ARRAY>false</IS_ARRAY>
        <GENERATE_SECURE>false</GENERATE_SECURE>
        <GENERATE_INSECURE>true</GENERATE_INSECURE>
    </COLUMN>


    <COLUMN name="contributionId">
        <IS_CONSTANT>false</IS_CONSTANT>
      <JAVADOC>/**
 * the contribution id corresponding to the contribution
 */</JAVADOC>
        <DB_TYPE>VARCHAR</DB_TYPE>
        <JAVA_TYPE>String</JAVA_TYPE>
        <USED_FOR_QUERY>true</USED_FOR_QUERY>
        <CAN_BE_NULL>false</CAN_BE_NULL>
        <IS_PRIMARY_KEY>false</IS_PRIMARY_KEY>
        <SIZE>255</SIZE>
        <IS_ARRAY>false</IS_ARRAY>
        <GENERATE_SECURE>false</GENERATE_SECURE>
        <GENERATE_INSECURE>true</GENERATE_INSECURE>
    </COLUMN>


    <COLUMN name="entryId">
        <IS_CONSTANT>false</IS_CONSTANT>
      <JAVADOC>/**
 * the entry id corresponding to the contribution
 */</JAVADOC>
        <DB_TYPE>VARCHAR</DB_TYPE>
        <JAVA_TYPE>String</JAVA_TYPE>
        <USED_FOR_QUERY>true</USED_FOR_QUERY>
        <CAN_BE_NULL>false</CAN_BE_NULL>
        <IS_PRIMARY_KEY>false</IS_PRIMARY_KEY>
        <SIZE>255</SIZE>
        <IS_ARRAY>false</IS_ARRAY>
        <GENERATE_SECURE>false</GENERATE_SECURE>
        <GENERATE_INSECURE>true</GENERATE_INSECURE>
    </COLUMN>


    <COLUMN name="date">
        <IS_CONSTANT>false</IS_CONSTANT>
      <JAVADOC>/**
 * the date of the action on the contribution
 */</JAVADOC>
        <DB_TYPE>TIMESTAMP</DB_TYPE>
        <JAVA_TYPE>java.sql.Timestamp</JAVA_TYPE>
        <USED_FOR_QUERY>true</USED_FOR_QUERY>
        <CAN_BE_NULL>false</CAN_BE_NULL>
        <IS_PRIMARY_KEY>false</IS_PRIMARY_KEY>
        <IS_ARRAY>false</IS_ARRAY>
        <GENERATE_SECURE>false</GENERATE_SECURE>
        <GENERATE_INSECURE>true</GENERATE_INSECURE>
    </COLUMN>


    <COLUMN name="status">
        <IS_CONSTANT>false</IS_CONSTANT>
      <JAVADOC>/**
 * the status of the contribution
 */</JAVADOC>
        <DB_TYPE>VARCHAR</DB_TYPE>
        <JAVA_TYPE>String</JAVA_TYPE>
        <USED_FOR_QUERY>true</USED_FOR_QUERY>
        <CAN_BE_NULL>false</CAN_BE_NULL>
        <IS_PRIMARY_KEY>false</IS_PRIMARY_KEY>
        <SIZE>255</SIZE>
        <IS_ARRAY>false</IS_ARRAY>
        <GENERATE_SECURE>false</GENERATE_SECURE>
        <GENERATE_INSECURE>true</GENERATE_INSECURE>
    </COLUMN>


    <INDEX id="author_idx" unique="false" clustered="false">
        <INDEX_COLUMN id="author"/>
    </INDEX>

    <INDEX id="volume_idx" unique="false" clustered="false">
        <INDEX_COLUMN id="volume"/>
    </INDEX>

    <INDEX id="headword_idx" unique="false" clustered="false">
        <INDEX_COLUMN id="headword"/>
    </INDEX>

    <INDEX id="contributionid_idx" unique="false" clustered="false">
        <INDEX_COLUMN id="contributionId"/>
    </INDEX>

    <INDEX id="entryid_idx" unique="false" clustered="false">
        <INDEX_COLUMN id="entryId"/>
    </INDEX>

    <INDEX id="status_idx" unique="false" clustered="false">
        <INDEX_COLUMN id="status"/>
    </INDEX>


</TABLE>
